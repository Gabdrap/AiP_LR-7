#include <iostream>
#include <Windows.h>

using namespace std;
bool a;
bool b;
bool c;
bool r1;

int main()
{
    SetConsoleCP(1251);
    SetConsoleOutputCP(1251);
    int choise;
    cout << "1" "\n";
    cout << "2" "\n";
    cout << "3" "\n";
    cout << "Выберите задание" "\n";
    cin >> choise;

    if (choise == 1)
    {
        cout << "Введите значение A: ";
        cin >> a;
        cout << "Введите значение B: ";
        cin >> b;
        cout << "Введите значение C: ";
        cin >> c;
        bool r1 = a || b && c;
        if (r1 == true)
        {
            cout << "Значение выражения: True";
        }
        else
        {
            cout << "Значение выражения: False";
        }
    }
}
